<h1>Listing cars</h1>

<table>
  <tr>
    <th>Nammanufacture</th>
    <th>Type</th>
    <th>Registrationnumber</th>
    <th>Dimensions</th>
    <th>Auto</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cars.each do |car| %>
  <tr>
    <td><%= car.nammanufacture %></td>
    <td><%= car.type %></td>
    <td><%= car.registrationnumber %></td>
    <td><%= car.dimensions %></td>
    <td><%= car.auto %></td>
    <td><%= link_to 'Show', car %></td>
    <td><%= link_to 'Edit', edit_car_path(car) %></td>
    <td><%= link_to 'Destroy', car, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Car', new_car_path %>
